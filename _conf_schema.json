{
    "user_config": {
        "description": "用户消息触发",
        "type": "object",
        "hint": "",
        "items": {
            "user_min_msg_len": {
                "description": "用户消息最小长度限制",
                "type": "int",
                "hint": "不超过此长度时不进行图库匹配",
                "default": 1
            },
            "user_max_msg_len": {
                "description": "用户消息最大长度限制",
                "type": "int",
                "hint": "超过此长度时不进行图库匹配",
                "default": 20
            },
            "user_exact_prob": {
                "description": "精准匹配时发图概率",
                "type": "float",
                "hint": "",
                "default": 0.9
            },
            "user_fuzzy_prob": {
                "description": "模糊匹配时发图概率",
                "type": "float",
                "hint": "",
                "default": 0.9
            }
        }
    },
    "llm_config": {
        "description": "LLM消息触发",
        "type": "object",
        "hint": "",
        "items": {
            "llm_min_msg_len": {
                "description": "LLM消息最小长度限制",
                "type": "int",
                "hint": "不超过此长度时不进行图库匹配",
                "default": 1
            },
            "llm_max_msg_len": {
                "description": "LLM消息最大长度限制",
                "type": "int",
                "hint": "超过此长度时不进行图库匹配",
                "default": 20
            },
            "llm_exact_prob": {
                "description": "精准匹配LLM消息时的发图概率",
                "type": "float",
                "hint": "",
                "default": 0.9
            },
            "llm_fuzzy_prob": {
                "description": "模糊匹配LLM消息时的发图概率",
                "type": "float",
                "hint": "",
                "default": 0.9
            }
        }
    },
    "add_default_config": {
        "description": "新建图库时的默认参数",
        "type": "object",
        "hint": "",
        "items": {
            "default_compress_switch": {
                "description": "新建图库时自动打开压缩开关",
                "type": "bool",
                "hint": "往新图库存图时，若图片尺寸大于压缩阈值则压缩图片",
                "default": true
            },
            "compress_size": {
                "description": "压缩阈值",
                "type": "int",
                "hint": "单位为像素，图片在512像素以下时qq以小图显示",
                "default": 512
            },
            "default_duplicate_switch": {
                "description": "新建图库时自动打开去重开关",
                "type": "bool",
                "hint": "往新图库存图时，若存在重复图片则终止操作",
                "default": true
            },
            "default_fuzzy_match": {
                "description": "新建图库时自动设置为模糊匹配",
                "type": "bool",
                "hint": "",
                "default": false
            },
            "label_max_length": {
                "description": "新建图库名最大长度",
                "type": "int",
                "hint": "单位为字符，超过此长度会被截断",
                "default": 10
            },
            "max_pic_count": {
                "description": "图库的默认容量上限",
                "type": "int",
                "hint": "图库中的图片数量达到此数量时，图库将无法添加图片",
                "default": 200
            }
        }
    },
    "perm_config": {
        "description": "权限设置",
        "type": "object",
        "hint": "以下是公共图库的权限设置，个人图库默认只能由本人或管理员操作",
        "items": {
            "allow_add": {
                "description": "允许非管理员向公共图库添加图片",
                "type": "bool",
                "hint": "图库的图片太少时建议打开，不过要小心被别人塞进不好的图片",
                "default": true
            },
            "allow_del": {
                "description": "允许非管理员删除公共图库的图片",
                "type": "bool",
                "hint": "建议关闭",
                "default": false
            },
            "allow_view": {
                "description": "允许非管理员查看公共图库的图片",
                "type": "bool",
                "hint": "建议打开",
                "default": true
            }
        }
    }
}