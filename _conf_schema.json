{
    "min_msg_length": {
        "description": "用户消息最小长度",
        "type": "int",
        "hint": "不超过此长度时不进行图库匹配",
        "default": 1
    },
    "max_msg_length": {
        "description": "用户消息最大长度",
        "type": "int",
        "hint": "超过此长度时不进行图库匹配",
        "default": 20
    },
    "default_compress_switch": {
        "description": "存图时是否默认压缩图片",
        "type": "bool",
        "hint": "不超过此长度时不响应",
        "default": true
    },
    "compress_size": {
        "description": "压缩阈值",
        "type": "int",
        "hint": "单位为像素，图片在512像素以下时qq以小图显示",
        "default": 512
    },
    "default_duplicate_switch": {
        "description": "是否允许图库中图片重复",
        "type": "bool",
        "hint": "",
        "default": false
    },
    "default_fuzzy_match": {
        "description": "是否默认模糊匹配",
        "type": "bool",
        "hint": "",
        "default": false
    },
    "exact_match_prob": {
        "description": "# 精准匹配时发图概率",
        "type": "int",
        "hint": "",
        "default": 0.9
    },
    "fuzzy_match_prob": {
        "description": "# 模糊匹配时发图概率",
        "type": "int",
        "hint": "",
        "default": 0.9
    },
    "label_max_length": {
        "description": "图库名最大长度()",
        "type": "int",
        "hint": "汉字为单位，一个字母相当于0.5714个汉字",
        "default": 4
    },
    "max_pic_count": {
        "description": "图库的默认容量上限",
        "type": "int",
        "hint": "图库中的图片数量不能超过此值",
        "default": 1
    }

}